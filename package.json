    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn lint"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "husky": "^4.3.0",
    "standard": "^14.3.4"
